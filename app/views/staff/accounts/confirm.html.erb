<% @title = 'アカウント情報編集（確認)' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @staff_member, url: :confirm_staff_account do |f| %>
    <p>以下の内容でアカウントを更新します。よろしいですか？</p>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '更新' %>
      <%= f.submit '訂正', name: 'corerct' %>
    </div>
  <% end %>
</div>
